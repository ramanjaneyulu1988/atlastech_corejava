package oops.encapsulation;

class Employee
{
	public String fname;
	public String lname;
	public int age;
	public String address;
	public String department;
	
	
}

public class EncapsulationExmaple1 {

	public static void main(String[] args) {
		
		Employee e = new Employee();
		
		e.address = "Blr";
		System.out.println(e.address);
	}
}
