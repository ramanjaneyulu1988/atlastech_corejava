package exception;

public class Example2 {

	public static void main(String[] args) {
		
		String s = null;
		try
		{
			System.out.println(s.concat("abc"));
		}
		catch(NullPointerException ex)
		{
			ex.printStackTrace();
		}
		
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		
	}
}
