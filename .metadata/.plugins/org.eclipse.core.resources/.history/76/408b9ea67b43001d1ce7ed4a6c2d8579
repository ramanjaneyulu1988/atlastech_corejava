package exception;

public class Example3 {

	public static void main(String[] args) {
		
		String s = "Bangalore";
		
		try
		{
			char c = s.charAt(20); // StringIndexOutOfBoundsException
			System.out.println(c);
		}
		catch(StringIndexOutOfBoundsException e) // Arithmetic
		{
			e.printStackTrace();
		}
		
		System.out.println("main method end...");
		
	}
}
