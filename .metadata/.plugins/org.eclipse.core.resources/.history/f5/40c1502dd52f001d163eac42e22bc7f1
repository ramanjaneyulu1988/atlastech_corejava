package com;

public class SIBExample1 {

	public static String department = "Blr";
	public static String name;
	public static String address;
	public static int age;
	
	public static void main(String[] args) {
		
		System.out.println("Main method");
	}
	
	static
	{
		department = "IT";
		name = "Alex";
		address = "Hyd";
		age = 30;
	}

}
