package exception;

public class Example3 {

	public static void main(String[] args) {
		
		String s = "Bangalore";
		
		try
		{
			char c = s.charAt(20); //
			System.out.println(c); // n
		}
		catch (RuntimeException e) {
			e.printStackTrace();
		}
		
		System.out.println("main method end...");
		
	}
}
