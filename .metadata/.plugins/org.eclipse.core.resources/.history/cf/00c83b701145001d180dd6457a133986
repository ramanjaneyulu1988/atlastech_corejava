package exception.compiletime;

import java.io.File;
import java.io.FileWriter;
import java.io.IOException;

public class Example2 {

	public static void main(String[] args) {
		
		File file = new File("D:/files"); // Creating Object for file class
		try {
			FileWriter fileWriter = new FileWriter(file+"/empnames.txt"); // opening connection for FileWriter
			fileWriter.write("Ram\nRajesh\nRakesh\nAlex\nRaj"); // We are writinf data into the file
			fileWriter.flush(); // flush is equal to in database commit
			fileWriter.close();
			
		} catch (IOException e) {
			e.printStackTrace();
		}
		
		System.out.println("main method end...");
	}
}
