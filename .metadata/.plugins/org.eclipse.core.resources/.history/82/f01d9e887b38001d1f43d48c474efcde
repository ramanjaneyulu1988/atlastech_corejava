package datatypes;

public class Example1 {

	public static byte b; // 0
	public static short s; // 0
	public static int i = 30; // 0
	public static long l; // 0
	public static float f; // 0.0
	public static double d; // 0.0
	public static boolean b1; // false
	public static char c; // single char space
	
	public static void main(String[] args) {
		
		System.out.println(Example1.b);
		System.out.println(Example1.s);
		System.out.println(Example1.i);
		System.out.println(Example1.l);
		System.out.println(Example1.f);
		System.out.println(Example1.d);
		System.out.println(Example1.b1);
		System.out.println(Example1.c);
		
	}
}
