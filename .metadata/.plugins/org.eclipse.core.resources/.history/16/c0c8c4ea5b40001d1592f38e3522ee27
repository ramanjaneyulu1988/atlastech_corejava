package exception;

public class Example2 {

	public static void main(String[] args) {
		
		String s = null;
		try
		{
			String s1 = s.concat("xyz");
			System.out.println(s1);
		}
		catch(NullPointerException e)
		{
			e.printStackTrace();
		}
		
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		System.out.println("main method end");
		
	}
}
