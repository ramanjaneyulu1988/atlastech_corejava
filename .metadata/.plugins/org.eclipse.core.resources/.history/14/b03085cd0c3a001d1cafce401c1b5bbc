package oops.polymorphism;

class City
{
	public void getCityName()
	{
		System.out.println("Bangalore");
	}
	
	// Bengaluru
}


class City1 extends City
{
	public void getCityName()
	{
		System.out.println("Bengaluru");
	}
}


public class MethodOveridingExample2 {

	public static void main(String[] args) {
		
		City c = new City1();
		
		c.getCityName();
	}
}
