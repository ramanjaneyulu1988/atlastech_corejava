package array;

public class Example1 {

	public static void main(String[] args) {
		
		// int array
		int i[] = {10,30,40,5,8,20};
		
		// or
		
		int[] j = {10,30,40,5,8,20};
		
		int k[] = new int[5];
		
		k[0] = 100;
		k[1] = 100;
		k[2] = 100;
		k[3] = 100;
		k[4] = 100;
		
		System.out.println(k);
		
		String s[] = {"abc", "xyz", "Ram", "100"};
		
		// Array is a collection of similar types
		
		
	}
}
