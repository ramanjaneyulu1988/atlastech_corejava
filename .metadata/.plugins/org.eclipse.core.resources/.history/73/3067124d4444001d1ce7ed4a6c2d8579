package exception;

public class Example8 {

	public static void main(String[] args) {
		
		String age = "abc";
		
		try
		{
			int i = Integer.parseInt(age);
			System.out.println(i);
		}
		catch(NumberFormatException e)
		{
			e.printStackTrace();
		}
		
		System.out.println("main method end..");
		
	}
}
